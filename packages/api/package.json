{
  "name": "api",
  "version": "1.0.0",
  "description": "LearningYogi timetable OCR pipeline API",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --transpile-only --respawn --project tsconfig.json src/main.ts",
    "start:debug": "node --inspect=0.0.0.0:9229 dist/main.js",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed:timetable": "ts-node -r tsconfig-paths/register src/scripts/seed-timetable.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.933.0",
    "@aws-sdk/lib-storage": "^3.933.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.20",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "joi": "^17.13.3",
    "mongoose": "^8.20.0",
    "multer": "^2.0.2",
    "openai": "^6.9.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.20",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
